{
  "name": "suomifi-ui-components",
  "version": "1.0.0",
  "description": "Suomi.fi UI component library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrk-kpa/suomifi-ui-components.git"
  },
  "author": "Joonas Kallunki <jkallunki@gmail.com> (https://jkallunki.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vrk-kpa/suomifi-ui-components/issues"
  },
  "homepage": "https://github.com/vrk-kpa/suomifi-ui-components#readme",
  "scripts": {
    "start": "webpack --env.development",
    "test": "nyc mocha -r ./babel-register.js 'src/**/*.test.tsx'",
    "test:coverage": "nyc check-coverage --statements 80 --branches 80 --functions 80 --lines 80",
    "test:lint": "tslint 'src/**/*.tsx'",
    "prebuild": "rimraf dist",
    "build": "webpack --env.production",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-transform-react-jsx": "7.1.6",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.0.0",
    "@storybook/addon-info": "4.0.7",
    "@storybook/addon-knobs": "4.0.7",
    "@storybook/react": "4.0.7",
    "@types/chai": "4.1.7",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/mocha": "5.2.5",
    "@types/storybook__addon-info": "3.4.2",
    "@types/storybook__react": "4.0.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.0.0-beta.6",
    "babel-plugin-istanbul": "5.1.0",
    "chai": "4.2.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "husky": "1.1.4",
    "lint-staged": "8.0.5",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "prettier": "1.15.2",
    "react-docgen-typescript-loader": "3.0.0",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "rimraf": "2.6.2",
    "storybook-addon-jsx": "5.4.0",
    "ts-loader": "5.3.0",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.16.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0",
    "tslint-webpack-plugin": "1.2.2",
    "typescript": "3.1.6",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "@types/react": "16.7.6",
    "@types/react-dom": "16.0.9",
    "react": "16.6.3",
    "react-dom": "16.6.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "src/**/*.d.ts",
      "src/**/*.test.ts",
      "src/**/*.test.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:coverage"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
