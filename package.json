{
  "name": "suomifi-ui-components",
  "version": "1.0.0",
  "description": "Suomi.fi UI component library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrk-kpa/suomifi-ui-components.git"
  },
  "author": "Joonas Kallunki <jkallunki@gmail.com> (https://jkallunki.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vrk-kpa/suomifi-ui-components/issues"
  },
  "homepage": "https://github.com/vrk-kpa/suomifi-ui-components#readme",
  "scripts": {
    "start": "yarn storybook",
    "start:webpack": "webpack --env.development",
    "test": "jest",
    "test:lint": "tslint 'src/**/*.tsx'",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prettier:check": "prettier-check 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "validate": "yarn test && yarn test:lint && yarn prettier:check",
    "prebuild": "rimraf dist",
    "build": "webpack --env.production",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006 -c .storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@storybook/addon-info": "^4.0.7",
    "@storybook/addon-knobs": "^4.0.7",
    "@storybook/react": "^4.0.7",
    "@types/jest": "^23.3.9",
    "@types/storybook__addon-info": "^3.4.2",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "husky": "^1.1.4",
    "jest": "^23.6.0",
    "jest-dom": "^2.1.1",
    "lint-staged": "^8.0.5",
    "prettier": "^1.15.2",
    "prettier-check": "^2.0.0",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-testing-library": "^5.3.0",
    "rimraf": "^2.6.2",
    "storybook-addon-jsx": "^5.4.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "tslint-webpack-plugin": "^1.2.2",
    "typescript": "^3.1.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn validate"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier-check"
    ]
  }
}
